
<RadzenStack Orientation="Orientation.Vertical" Gap="20">
    <HeaterDisplays/>
    <VoltCurrentDisplay LabelPrefix="V" Values="@this.Voltages"/>
    <VoltCurrentDisplay LabelPrefix="I" Values="@this.Currents"/>
