                              Title="Middle Pocket"
                              LineType="LineType.Solid"
                              ValueProperty="T3">
                <RadzenMarkers Visible="true" MarkerType="MarkerType.Circle"/>
                <RadzenSeriesDataLabels Visible="false"/>
            </RadzenLineSeries>
            <RadzenCategoryAxis LineType="LineType.Solid">
                <RadzenAxisTitle Text="Time(s)"/>
                <RadzenGridLines Visible="true" StrokeWidth="3"/>
            </RadzenCategoryAxis>
            <RadzenValueAxis >
                <RadzenGridLines Visible="true" StrokeWidth="3"/>
                <RadzenAxisTitle Text="Temperature(C)"/>
            </RadzenValueAxis>
        </RadzenChart>
    </RadzenStack>
</RadzenStack>


@code {
    [Parameter] public IEnumerable<StationTestReading>? StationTestLogs { get; set; }
    
}